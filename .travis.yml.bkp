language: python
python:
- 3.6
sudo: required
env:
  global:
  - TRAVIS_CI_OPERATOR=1
  - secure: 
  - secure: 
  - secure: 
services:
- docker
install:
- pip install transifex-client
- bash .travis.sh install
script:
- tx push --source
- tx pull --mode sourceastranslation -l ar,en 
- bash .travis.sh script
deploy:
  skip_cleanup: true
  provider: script
  script: bash .travis.sh deploy
  on:
    all_branches: false
    condition: $TRAVIS_PULL_REQUEST = "false"
